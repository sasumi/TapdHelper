{
  "manifest_version": 2,
  "name": "TapdFreeMan",
  "version": "1.0",
  "description": "tapd bug tickets optimize plugin",
  "author": "sasumi",
  "icons": {
	"48": "icon.png",
	"128": "icon.png"
  },
  "homepage_url": "http://blog.yikepi.com",
  "permissions": [
	"chrome://favicon/",
	"http://*/",
	"https://*/"
  ],
  "options_ui": {
	"page": "options.html",
	"chrome_style": true
  },
  "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  "content_scripts": [
	{
	  "matches": [
		"https://www.tapd.cn/*"
	  ],
	  "css": [
		"assert/css/content.css"
	  ],
	  "js": [
		"assert/js/chrome.js",
		"assert/js/jquery-1.8.3.min.js",
		"assert/js/echarts.min.js",
		"assert/js/content.base.js",
		"assert/js/content.run.js"
	  ],
	  "web_accessible_resources": [
		"assert/img/icon.png"
	  ],
	  "run_at": "document_end"
	}
  ],
  "background": {
	"scripts": [
	  "assert/js/chrome.js",
	  "assert/js/background.js",
	  "assert/js/jquery-1.8.3.min.js"
	]
  },
  "web_accessible_resources": [
  ]
}